          <expression id="571f0c2d-ef49-4a41-83d4-79028e7d9818">
            <annotation></annotation>
            <name>Result Field: returnResult</name>
            <fieldName>returnResult</fieldName>
            <sourceType>RAWRESULT</sourceType>
            <filters>
              <scriptletFilter id="9e373344-f21b-4525-ae32-1ab104f67459">
                <annotation></annotation>
                <name>Scriptlet</name>
                <scriptlet id="8c3c481b-0f5c-4b4b-a1fb-ba6ec9d62fa2">
                  <annotation></annotation>
                  <name>default</name>
                  <script>
//-------------------------------------------------------------------
// find the key &quot;config.network.vnic&quot;
var vnicKey=&quot;config.network.vnic&quot;;
data = String(scriptletInput);
var i = data.indexOf(vnicKey);
var ipAddressValue = null;
if( i != -1 )
{
// OK. found 
   // Find the array starting symbol: [
   i = data.indexOf(&quot;[&quot;);
   if( i != -1 ) {
	// truncate.
	data = data.slice( i + 1 );
	
	var count = 0;
	var start=-1, close = -1;
	var fromIndex = 0;
	
		for ( i = 0; i &lt; data.length; i ++ ) {
			ch = data.charAt(i);
			
			if( ch == &apos;{&apos;){
				count ++;
				if( start == -1 ) start = i;
			}else if ( ch == &apos;}&apos;) {
				count --;
				close = i;
				
				if( count == 0 ) {
				 // Found a entry. 
				 
					entry = data.slice(start + 1, close );
					// Check if this entry is for the Management Network.
					mgmtIndex = entry.indexOf(&quot;Management Network&quot;);
					if( mgmtIndex != -1 ) {
						// Great We found it.
						//Get the Ip address field:
						ipAddressConst = &quot;ipAddress&quot;;
						ipAddrIndex = entry.indexOf(ipAddressConst);
						if(ipAddrIndex != -1 )
						{
							// Get the value closing symbol &quot;,&quot;.
							valueEndIndex = entry.indexOf(&quot;,&quot;, ipAddrIndex);
							if( valueEndIndex != -1 )
							{
								// slice out the ipAddress Value.
								ipAddressValue = entry.slice( ipAddrIndex + ipAddressConst.length + 1, valueEndIndex);
								
							}
						}
						
						break;
					
					}else {
						// Well not this entry. Let&apos;s try the next.
						
						//Reset start.
						start = -1;
						
					}
				 }
				
			}
			
			
		
		}
		
	
   }else {
     // well, data error.
   }
}else {
// well, no matching
}
scriptletResult = ipAddressValue;
//----------------------------------------------------------------
//console.log(&quot;Found address: [%s]&quot;, ipAddressValue);</script>
                  <type>RHINO</type>
                </scriptlet>
              </scriptletFilter>
            </filters>
          </expression>
        </flowVariableAssignment>
