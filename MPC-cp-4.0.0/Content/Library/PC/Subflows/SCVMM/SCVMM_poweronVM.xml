      <scriptlet id="a0ce47f9-3e2d-4b63-9510-7bfccf108ed7">
        <annotation></annotation>
        <name>Step Set Flow Variable in Flow SCVMM_poweronVM (24deb7c3-72e1-4d0e-b1bb-ad0b18f3ec52)</name>
        <script>
var vmName=scriptletContext.getLocal(&quot;vmName&quot;);

var commands = [];

commands.push(&quot; $myVM = Get-SCVirtualMachine -Name \&quot;&quot; + vmName + &quot;\&quot;&quot; );

commands.push(&quot;if ( $myVM.Status -eq &apos;PowerOff&apos; ) { &quot;);
commands.push(&quot;    Start-SCVirtualMachine -VM $myVM &quot;);
commands.push(&quot; }&quot;);

scriptletContext.put(&quot;commands&quot;,commands.join(&quot;\r\n&quot;));</script>
        <type>RHINO</type>
