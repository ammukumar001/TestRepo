      <scriptlet id="64a963d6-310b-406a-904f-4f4417f31f52">
        <annotation></annotation>
        <name>Step Set Flow Variable in Flow SCVMM_poweroffVM (2fe712d6-5d85-4585-b47a-33eaeb1e2120)</name>
        <script>
var vmName=scriptletContext.getLocal(&quot;vmName&quot;);

var commands = [];

commands.push(&quot; $myVM = Get-SCVirtualMachine -Name \&quot;&quot; + vmName + &quot;\&quot;&quot; );

commands.push(&quot;if ( $myVM.Status -eq &apos;Running&apos; ) { &quot;);
commands.push(&quot;    Stop-SCVirtualMachine -VM $myVM &quot;);
commands.push(&quot; }&quot;);

scriptletContext.put(&quot;commands&quot;,commands.join(&quot;\r\n&quot;));</script>
        <type>RHINO</type>
