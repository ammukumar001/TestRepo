      <scriptlet id="b0a85a33-bcbe-4553-a8fb-c973d83ff2f9">
        <annotation></annotation>
        <name>Step Set Flow Variable in Flow SCVMM_addSnapshot (6282a094-4142-493c-8c8b-79765f849102)</name>
        <script>
/////  vlan params
var vmName=scriptletContext.getLocal(&quot;vmName&quot;);
var snapshotName = scriptletContext.getLocal(&quot;addSnapshotName&quot;);
var commands = [];

commands.push(&quot; $myVM = Get-SCVirtualMachine -Name \&quot;&quot; + vmName + &quot;\&quot;&quot; );
commands.push(&quot; Get-SCVMCheckpoint  -VM $myvm | Remove-SCVMCheckpoint &quot;);
commands.push(&quot; $snapshotName = \&quot;&quot; + snapshotName + &quot;\&quot;&quot;);
commands.push(&quot; New-SCVMCheckpoint -VM $myvm -Name $snapshotName -Description \&quot;Checkpoint created by MPC automation\&quot;&quot;);

scriptletContext.put(&quot;commands&quot;,commands.join(&quot;\r\n&quot;));</script>
        <type>RHINO</type>
      </scriptlet>
