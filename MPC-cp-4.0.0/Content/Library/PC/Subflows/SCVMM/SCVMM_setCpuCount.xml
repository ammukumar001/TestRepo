      <scriptlet id="cd2c311c-3b0a-4166-828b-1dd2921ca01a">
        <annotation></annotation>
        <name>Step Set Flow Variable in Flow SCVMM_setCpuCount (f79e7fd1-cf2e-427d-93c8-4e72a3fb36c5)</name>
        <script>
/////  vlan params
var vmName=scriptletContext.getLocal(&quot;vmName&quot;);
var cpuCounts = scriptletContext.getLocal(&quot;cpuCounts&quot;);
var commands = [];

commands.push(&quot; $VM = Get-SCVirtualMachine | where { $_.Name -eq \&quot;&quot; + vmName + &quot;\&quot; }&quot;);
commands.push(&quot;if($VM.Status -ne &apos;PowerOff&apos;){Stop-SCVirtualMachine -VM $VM}&quot;);
commands.push(&quot; Set-SCVirtualMachine -VM $VM -CPUCount &quot; + cpuCounts + &quot;&quot;);

scriptletContext.put(&quot;commands&quot;,commands.join(&quot;\r\n&quot;));</script>
        <type>RHINO</type>
      </scriptlet>
